{
  "name": "auth-server",
  "version": "1.0.0",
  "license": "MIT",
  "devDependencies": {
    "axios": "^0.16.2",
    "babel-eslint": "^7.2.3",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.5.0",
    "eslint-config-esayemm": "^2.0.9",
    "eslint-plugin-react": "^7.3.0",
    "expect": "^1.20.2",
    "jbs-node": "^0.0.3",
    "mocha": "^3.5.0"
  },
  "scripts": {
    "test": "APP_ENV=test ./node_modules/mocha/bin/mocha --compilers js:babel-register --require babel-polyfill $(find . -name '*.spec.js' ! -ipath '*node_modules*' ! -ipath '*dist*')",
    "test:watch": "APP_ENV=test ./node_modules/mocha/bin/mocha --compilers js:babel-register --require babel-polyfill --watch $(find . -name '*.spec.js' ! -ipath '*node_modules*' ! -ipath '*dist*')",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "build": "./node_modules/jbs-node/bin.js build --input src --output build",
    "deploy": "npm run build && echo add deployment script here",
    "start": "NODE_PATH=./src nodemon index.js | ./node_modules/bunyan/bin/bunyan --output short",
    "serve": "NODE_PATH=./build node ./build"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "bunyan": "^1.8.12",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "helmet": "^3.8.1",
    "js-functions": "^0.0.8",
    "morgan": "^1.8.2"
  },
  "main": "./build/index.js",
  "babel": {
    "presets": [
      "./node_modules/jbs-node/configs/babel-preset-jbs-node.js"
    ]
  }
}